{
  "command_timeout": 5,
  "templates_folder": "Vault/Templates",
  "templates_pairs": [
    [
      "",
      ""
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": true,
  "enable_system_commands": false,
  "shell_path": "",
  "user_scripts_folder": "",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "Longform notes",
      "template": "Vault/Templates/% new longform.md"
    },
    {
      "folder": "/",
      "template": "Vault/Templates/% new.md"
    },
    {
      "folder": "Academia/Problems",
      "template": "Vault/Templates/% problem.md"
    }
  ],
  "enable_file_templates": false,
  "file_templates": [
    {
      "regex": ".*",
      "template": ""
    }
  ],
  "syntax_highlighting": true,
  "syntax_highlighting_mobile": false,
  "enabled_templates_hotkeys": [
    "Vault/Templates/% lecture-note.md"
  ],
  "startup_templates": [
    ""
  ],
  "intellisense_render": 1,
  "enable_ribbon_icon": true
}